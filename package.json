{
  "name": "react-starter",
  "version": "0.0.0",
  "description": "",
  "private": true,
  "engines": {
    "node": "^10.15.0",
    "npm": "^6.0.0",
    "yarn": "^1.13.0"
  },
  "scripts": {
    "format": "prettier-standard 'src/**/**/*.js'",
    "start": "react-scripts start",
    "build": "rimraf build && react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "@commitlint/cli": "^7.5.2",
    "@commitlint/config-conventional": "^7.5.0",
    "husky": "^1.3.1",
    "prettier-standard": "^9.1.1",
    "lint-staged": "^8.1.3",
    "rimraf": "^2.6.3"
  },
  "dependencies": {
    "react": "^16.8.1",
    "react-dom": "^16.8.1",
    "react-router-dom": "^4.3.1",
    "react-scripts": "2.1.5"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "lint-staged": {
    "src/**/**/*.js": [
      "prettier-standard",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -e",
      "pre-commit": "lint-staged"
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "resolutions": {
    "typescript": "^3.0.0",
    "kleur": "^3.0.0"
  }
}
